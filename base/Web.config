<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns="http://schemas.microsoft.com/.NetConfiguration/v2.0">
  <appSettings>
    <!-- ログイン　横サイズ -->
    <add key="LoginWidthSize" value="1264" />
    <!-- ログイン　縦サイズ -->
    <add key="LoginHeightSize" value="660" />
    <!-- ログイン　画面位置調整 -->
    <add key="LoginPositioning" value="true" />
    <!-- ログイン　トップ位置調整サイズ -->
    <add key="LoginTopPositionAdjustment" value="" />
    <!-- ログイン　トップサイズ -->
    <add key="LoginPositionTop" value="0" />
    <!-- ログイン　レフトサイズ -->
    <add key="LoginPositionLeft" value="0" />
    <!-- ログイン　アラーム画面（起動用機能名） -->
    <add key="LoginFunctionID" value="TM030P01" />
    <!-- 2012/12/07 WT)K.Banno 変更管理[OM-0092] Add Start -->
    <!-- ログオフ　未決裁情報表示画面 （起動用機能名） -->
    <add key="LogoffFunctionID" value="" />
    <!-- 2012/12/07 WT)K.Banno 変更管理[OM-0092] Add  End  -->
    <add key="LogoffWindowCheck" value="true" />
    <!-- ログオフ　強制終了 -->
    <add key="LogoffForceQuit" value="false" />
    <!-- ログオフ時に強制クローズするファンクションID格納キー
      カンマ区切りで複数指定可能 -->
    <add key="LogoffClosingFunctionIDs" value="TH020P01" />
    <!-- ログイン画面情報表示 -->
    <add key="DefaultContent" value="default.html" />
    <!-- メニューキャシュ -->
    <add key="MenuCacheApp" value="false" />
    <!-- ログイン　起動システム名 -->
    <add key="LoginSystemName" value="MD_SYSTEM" />
    <!-- ログイン　運用時間チェック -->
    <add key="LoginOperationCheck" value="false" />
    <!-- ポータルマイレージ表示ＷＳ   -->
    <add key="PortalWsMode" value="false" />
    <add key="Portal.portalws" value="http://localhost/portalws/portalws.asmx" />
    <!-- ポータルマイレージ表示ＷＳ   -->
    <add key="PortalWsMode" value="false" />
    <!-- インストールされているローカルパス   -->
    <add key="BaseInstallPath" value="D:\\xebio\\" />
    <!-- メニュー形式   -->
    <add key="BaseMenuFormat" value="GS" />
  </appSettings>
  <connectionStrings />
  <system.net>
    <defaultProxy>
      <proxy usesystemdefault="false" />
    </defaultProxy>
  </system.net>
  <system.serviceModel>
    <client>
      <!-- データ管理基盤の基本サービスを使用するためのエンドポイント -->
      <endpoint address="net.pipe://localhost/DataMgr/DataMgr" binding="netNamedPipeBinding" bindingConfiguration="netNamedPipeBindingCfg" contract="Com.Fujitsu.SmartBase.DataIntegration.WcfClient.DataMgrLib.Inneruse.IDataMgrService" name="sys_dataMgr_endpoint" />
      <!-- データ管理基盤の会計関連サービスを使用するためのエンドポイント -->
      <endpoint address="net.pipe://localhost/DataMgr/Accounting" binding="netNamedPipeBinding" bindingConfiguration="netNamedPipeBindingCfg" contract="Com.Fujitsu.SmartBase.DataIntegration.WcfClient.DataMgrLib.Inneruse.IAccountingService" name="sys_accounting_endpoint" />
      <!-- データ管理基盤の管理者用サービスを使用するためのエンドポイント -->
      <endpoint address="net.pipe://localhost/DataMgr/Admin" binding="netNamedPipeBinding" bindingConfiguration="netNamedPipeBindingCfg" contract="Com.Fujitsu.SmartBase.DataIntegration.WcfClient.DataMgrLib.Inneruse.IAdminService" name="sys_admin_endpoint" />
      <!-- データ管理基盤からイベント通知基盤の管理サービスを使用するためのエンドポイント -->
      <endpoint address="net.pipe://localhost/Eventing/Admin" binding="netNamedPipeBinding" bindingConfiguration="smartSoa_to_subscription" contract="Com.Fujitsu.SmartBase.DataIntegration.WcfClient.Eventing.IAdminService" name="sys_eventing_admin_endpoint" />
      <!-- データ管理基盤のリポジトリ管理サービスを使用するためのエンドポイント -->
      <endpoint address="net.pipe://localhost/DataMgr/RepositoryMgr" binding="netNamedPipeBinding" bindingConfiguration="netNamedPipeBindingCfg" contract="Com.Fujitsu.SmartBase.DataIntegration.WcfClient.DataMgrLib.Inneruse.IRepositoryService" name="sys_repository_endpoint" />
    </client>
    <bindings>
      <netNamedPipeBinding>
        <binding name="netNamedPipeBindingCfg" closeTimeout="00:01:00" openTimeout="00:01:00" receiveTimeout="01:00:00" sendTimeout="01:00:00" hostNameComparisonMode="StrongWildcard" maxBufferSize="65536" maxBufferPoolSize="524288" maxReceivedMessageSize="314572800" transferMode="Streamed">
          <readerQuotas maxDepth="32" maxStringContentLength="314572800" maxArrayLength="16384" maxBytesPerRead="4096" maxNameTableCharCount="16384" />
          <security mode="None" />
        </binding>
        <binding name="smartSoa_to_subscription" closeTimeout="00:01:00" openTimeout="00:01:00" receiveTimeout="00:10:00" sendTimeout="00:01:00" hostNameComparisonMode="StrongWildcard" maxBufferSize="65536" maxBufferPoolSize="524288" maxReceivedMessageSize="65536" transferMode="Buffered">
          <readerQuotas maxDepth="32" maxStringContentLength="8192" maxArrayLength="16384" maxBytesPerRead="4096" maxNameTableCharCount="16384" />
          <security mode="None" />
        </binding>
      </netNamedPipeBinding>
    </bindings>
  </system.serviceModel>
